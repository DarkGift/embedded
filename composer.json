{
    "name": "DarkGift/embedded",
    "description": "Provides support for embedded (nested) models in Yii2",
    "keywords": ["yii2", "embed", "embedded", "nested", "mongo", "mongodb", "elastic", "elasticsearch"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/DarkGift/embedded/issues",
        "forum": "http://www.DarkGift.com/forum/",
        "wiki": "https://github.com/yii2tech/embedded/wiki",
        "source": "https://github.com/DarkGift/embedded"
    },
    "authors": [
        {
            "name": "Pavel Terletskiy",
            "email": "pavelterletskiy@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": { "DarkGift\\embedded\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
